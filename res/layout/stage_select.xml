        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/dialog"
        android:layout_alignLeft="@+id/dialog"
        android:layout_marginTop="37dp"
        android:layout_marginLeft="30dp"
        android:text="你說的是"
        android:textColor="#000000"
